(define (f n m) 
  (cond ((or (= n 1) 
             (= m 1) 
             (= n m)) 
         1)
        (else (+ (f (- n 1) 
                    (- m 1))
                 (f (- n 1) 
                    m)))))
(f 1 1)
(f 2 1)
(f 2 2)
(f 3 1)
(f 3 2)
(f 3 3)
(f 4 1)
(f 4 2)
(f 4 3)
(f 4 4)
(f 5 1)
(f 5 2)
(f 5 3)
(f 5 4)
(f 5 5)

